{
  "code": "tensorboard",
  "index": 132,
  "name": "TensorBoard",
  "description": "TensorBoard is a tool for providing the measurements and visualizations needed during the machine learning workflow. It enables tracking experiment metrics like loss and accuracy, visualizing the model graph, projecting embeddings to a lower dimensional space, and much more.",
  "category": "AI",
  "featureCode": "ai",
  "display": true,
  "type": "system",
  "params": [
    {
      "id": "job_name",
      "name": {
        "en": "Job Name",
        "zh": "作业名称"
      },
      "class": "base",
      "dataType": "string",
      "must": true,
      "type": "system",
      "input": "input",
      "maxLength": 64
    },
    {
      "id": "job_workspace",
      "name": {
        "en": "Workspace",
        "zh": "工作目录"
      },
      "class": "base",
      "dataType": "folder",
      "defaultValue": "MyFolder",
      "must": true,
      "type": "system"
    },
    {
      "id": "image_path",
      "name": {
        "en": "Container Image",
        "zh": "容器镜像"
      },
      "class": "param",
      "dataType": "image",
      "hypervisor": "singularity",
      "framework": "tensorflow2,tensorflow",
      "defaultValue": "tensorflow2",
      "must": true
    },
    {
      "id": "log_dir",
      "name": {
        "en": "Log directory",
        "zh": "日志目录"
      },
      "class": "param",
      "dataType": "folder",
      "must": true,
      "input": "input"
    },
    {
      "id": "job_queue",
      "name": {
          "en": "Queue",
          "zh": "队列"
      },
      "class": "resource",
      "dataType": "queue",
      "must": true,
      "type": "system"
    },
    {  
      "id": "cores_per_node",
      "name": {
        "en": "CPU Cores Per Node",
        "zh": "每节点的CPU核数"
      },
      "class": "resource",
      "dataType": "number",
      "input": "input",
      "minValue": "1",
      "maxValue": "999",
      "floatLength": 0,
      "defaultValue": "2",
      "must": true
    },
    {
      "id": "run_time",
      "name": {
        "en": "Wall Time",
        "zh": "最大运行时间"
      },
      "class": "resource",
      "dataType": "string",
      "input": "input",
      "tips": {
          "type": "text",
          "content": "eg.3d 4h 12m"
      },
      "help": {
        "en": "Please input run time limit for the job,Format is 'Xd Xh Xm',which 'Xd' represents X days,'Xh' represents X hours and 'Xm' represents X minutes.",
        "zh": "请输入job的运行时间限制，格式为“Xd Xh Xm”，“Xd”表示X天，“Xh”表示X小时，“Xm”表示X分钟。"
      },
      "defaultValue": "",
      "must": false
    },
    {
      "id": "job_notify",
      "name": {
        "en": "Notify Job Completion",
        "zh": "作业完成通知"
      },
      "class": "notify",
      "dataType": "string",
      "input": "select",
      "selectOption": [
        {
          "label": {
            "en": "None",
            "zh": "无"
          },
          "value": ""
        },
        {
          "label": {
            "en": "Email",
            "zh": "邮件"
          },
          "value": "email"
        }
      ],
      "defaultValue": "",
      "must": false
    }
  ]
}
