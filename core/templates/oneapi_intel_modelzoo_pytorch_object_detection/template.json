      "dataType": "queue",
      "must": true,
      "type": "system"
    },
    {
      "id": "exclusive",
      "name": {
        "en": "Exclusive",
        "zh": "独占节点"
      },
      "class": "resource",
      "dataType": "boolean",
      "defaultValue": true,
      "must": false,
      "valueTrigger": "context.measurement_type!='online_inference'"
    },
    {
      "id": "cores_per_node",
      "name": {
        "en": "CPU Cores",
        "zh": "CPU核数"
      },
      "class": "resource",
      "dataType": "number",
      "input": "input",
      "minValue": "1",
      "maxValue": "999",
      "floatLength": 0,
      "defaultValue": "4",
      "must": true,
      "visible": "!context.exclusive"
    },
    {
      "id": "ram_size",
      "name": {
        "en": "Memory Used(MB)",
        "zh": "内存使用(MB)"
      },
      "class": "resource",
      "dataType": "number",
      "input": "input",
      "minValue": "1",
      "maxValue": "9999",
      "floatLength": 0,
      "defaultValue": "",
      "must": false
    },
    {
      "id": "run_time",
      "name": {
        "en": "Wall Time",
        "zh": "最大运行时间"
      },
      "class": "resource",
      "dataType": "string",
      "input": "input",
      "tips": {
        "type": "text",
        "content": "eg.3d 4h 12m"
      },
      "help": {
        "en": "Please input run time limit for the job,Format is 'Xd Xh Xm',which 'Xd' represents X days,'Xh' represents X hours and 'Xm' represents X minutes.",
        "zh": "请输入job的运行时间限制，格式为“Xd Xh Xm”，“Xd”表示X天，“Xh”表示X小时，“Xm”表示X分钟。"
      },
      "defaultValue": "24h",
      "must": false
    },
    {
      "id": "job_notify",
      "name": {
        "en": "Notify Job Completion",
        "zh": "作业完成通知"
      },
      "class": "notify",
      "dataType": "string",
      "input": "select",
      "selectOption": [
        {
          "label": {
            "en": "None",
            "zh": "无"
          },
          "value": ""
        },
        {
          "label": {
            "en": "Email",
            "zh": "邮件"
          },
          "value": "email"
        }
      ],
      "defaultValue": "",
      "must": false
    }
  ]
}

