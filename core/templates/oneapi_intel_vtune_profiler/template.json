{
  "code": "vtune",
  "index": 12,
  "name": "Intel VTune Profiler",
  "description": "Intel® VTune™ Profiler optimizes application performance, system performance, and system configuration for HPC, cloud, IoT, media, storage, and more.",
  "category": "Intel_oneAPI,HPC",
  "featureCode": "sc.host.*,oneapi",
  "display": true,
  "type": "system",
  "params": [
    {
      "id": "job_name",
      "name": {
        "en": "Job Name",
        "zh": "作业名称"
      },
      "class": "base",
      "dataType": "string",
      "must": true,
      "type": "system",
      "input": "input",
      "maxLength": 64
    },
    {
      "id": "job_workspace",
      "name": {
        "en": "Workspace",
        "zh": "工作目录"
      },
      "class": "base",
      "dataType": "folder",
      "must": true,
      "type": "system"
    },
    {
      "id": "job_queue",
      "name": {
        "en": "Queue",
        "zh": "队列"
      },
      "class": "resource",
      "dataType": "queue",
      "must": true,
      "type": "system"
    },
    {
      "id": "runtime_id",
      "name": {
        "en": "Runtime ENV",
        "zh": "运行环境"
      },
      "class": "param",
      "dataType": "runtime",
      "must": false,
      "defaultValue": "Intel_Performance_Analyzer"
    },
    {
      "id": "data_directory",
      "name": {
        "en": "Data Directory",
        "zh": "运行目录"
      },
      "class": "param",
      "dataType": "folder",
      "must": false
    },
    {
      "id": "cores_per_node",
      "name": {
        "en": "CPU Cores",
        "zh": "CPU核数"
      },
      "class": "resource",
      "dataType": "number",
      "input": "input",
      "minValue": "1",
      "maxValue": "999",
      "floatLength": 0,
      "defaultValue": "1",
      "must": true
    },
    {
      "id": "ram_size",
      "name": {
        "en": "Memory Used(MB)",
        "zh": "内存使用(MB)"
      },
      "class": "resource",
      "dataType": "number",
      "input": "input",
      "minValue": "1",
      "maxValue": "999999",
      "floatLength": 0,
      "defaultValue": "",
      "must": false
    },
    {
      "id": "run_time",
      "name": {
        "en": "Wall Time",
        "zh": "最大运行时间"
      },
      "class": "resource",
      "dataType": "string",
      "input": "input",
      "tips": {
        "type": "text",
        "content": "eg.3d 4h 12m"
      },
      "help": {
        "en": "Please input run time limit for the job,Format is 'Xd Xh Xm',which 'Xd' represents X days,'Xh' represents X hours and 'Xm' represents X minutes.",
        "zh": "请输入job的运行时间限制，格式为“Xd Xh Xm”，“Xd”表示X天，“Xh”表示X小时，“Xm”表示X分钟。"
      },
      "defaultValue": "24h",
      "must": false
    },
    {
      "id": "job_notify",
      "name": {
        "en": "Notify Job Completion",
        "zh": "作业完成通知"
      },
      "class": "notify",
      "dataType": "string",
      "input": "select",
      "selectOption": [
        {
          "label": {
            "en": "None",
            "zh": "无"
          },
          "value": ""
        },
        {
          "label": {
            "en": "Email",
            "zh": "邮件"
          },
          "value": "email"
        }
      ],
      "defaultValue": "",
      "must": false
    }
  ]
}
